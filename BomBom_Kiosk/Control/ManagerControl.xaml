<UserControl x:Class="BomBom_Kiosk.Control.ManagerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BomBom_Kiosk.Control"
             xmlns:ctrl="clr-namespace:BomBom_Kiosk.Control.Manager"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="메뉴">
                <ctrl:MenuManagerControl />
            </TabItem>
            <TabItem Header="회원">
                <ctrl:MemberManagerControl />
            </TabItem>
            <TabItem Header="통계">
                <ctrl:StatisticsControl />
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
 